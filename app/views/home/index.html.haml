!!!
%html{lang: "en"}
  %head
    %meta{content: "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{charset: "utf-8"}/
    %meta{content: "width=device-width, initial-scale=1.0", name: "viewport"}/
    %meta{content: "", name: "description"}/
    %meta{content: "", name: "author"}/
    %title Home | E-Shopper
    = stylesheet_link_tag 'bootstrap.min', media: 'all', 'data-turbolinks-track': 'reload' 
    = stylesheet_link_tag 'font-awesome.min', media: 'all'
    = stylesheet_link_tag 'prettyPhoto', media: 'all'
    = stylesheet_link_tag 'animate', media: 'all'
    = stylesheet_link_tag 'home', media: 'all'
    = stylesheet_link_tag 'responsive', media: 'all'
  / /head
  %body
    %header#header
      / header
      .header_top
        / header_top
        .container
          .row
            .col-sm-6
              .contactinfo
                %ul.nav.nav-pills
                  %li
                    %a{href: "#"}
                      %i.fa.fa-phone
                      +2 95 01 88 821
                  %li
                    %a{href: "#"}
                      %i.fa.fa-envelope
                      info@domain.com
            .col-sm-6
              .social-icons.pull-right
                %ul.nav.navbar-nav
                  %li
                    %a{href: "#"}
                      %i.fa.fa-facebook
                  %li
                    %a{href: "#"}
                      %i.fa.fa-twitter
                  %li
                    %a{href: "#"}
                      %i.fa.fa-linkedin
                  %li
                    %a{href: "#"}
                      %i.fa.fa-dribbble
                  %li
                    %a{href: "#"}
                      %i.fa.fa-google-plus
      / /header_top
      .header-middle
        / header-middle
        .container
          .row
            .col-sm-4
              .logo.pull-left
                %a{href: "index.html"}
                  = image_tag "home/logo.png", alt: "" 
              .btn-group.pull-right
                .btn-group
                  %button.btn.btn-default.dropdown-toggle.usa{"data-toggle" => "dropdown", type: "button"}
                    USA
                    %span.caret
                  %ul.dropdown-menu
                    %li
                      %a{href: "#"} Canada
                    %li
                      %a{href: "#"} UK
                .btn-group
                  %button.btn.btn-default.dropdown-toggle.usa{"data-toggle" => "dropdown", type: "button"}
                    DOLLAR
                    %span.caret
                  %ul.dropdown-menu
                    %li
                      %a{href: "#"} Canadian Dollar
                    %li
                      %a{href: "#"} Pound
            .col-sm-8
              .shop-menu.pull-right
                %ul.nav.navbar-nav
                  %li
                    %a{href: "#"}
                      %i.fa.fa-user
                      Account
                  %li
                    %a{href: "#"}
                      %i.fa.fa-star
                      Wishlist
                  %li
                    %a{href: "checkout.html"}
                      %i.fa.fa-crosshairs
                      Checkout
                  %li
                    %a{href: "cart.html"}
                      %i.fa.fa-shopping-cart
                      Cart
                  %li
                    %a{href: "login.html"}
                      %i.fa.fa-lock
                      Login
      / /header-middle
      .header-bottom
        / header-bottom
        .container
          .row
            .col-sm-9
              .navbar-header
                %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", type: "button"}
                  %span.sr-only Toggle navigation
                  %span.icon-bar
                  %span.icon-bar
                  %span.icon-bar
              .mainmenu.pull-left
                %ul.nav.navbar-nav.collapse.navbar-collapse
                  %li
                    %a.active{href: "index.html"} Home
                  %li.dropdown
                    %a{href: "#"}
                      Shop
                      %i.fa.fa-angle-down
                    %ul.sub-menu{role: "menu"}
                      %li
                        %a{href: "shop.html"} Products
                      %li
                        %a{href: "product-details.html"} Product Details
                      %li
                        %a{href: "checkout.html"} Checkout
                      %li
                        %a{href: "cart.html"} Cart
                      %li
                        %a{href: "login.html"} Login
                  %li.dropdown
                    %a{href: "#"}
                      Blog
                      %i.fa.fa-angle-down
                    %ul.sub-menu{role: "menu"}
                      %li
                        %a{href: "blog.html"} Blog List
                      %li
                        %a{href: "blog-single.html"} Blog Single
                  %li
                    %a{href: "404.html"} 404
                  %li
                    %a{href: "contact-us.html"} Contact
            .col-sm-3
              .search_box.pull-right
                %input{placeholder: "Search", type: "text"}/
      / /header-bottom
    / /header
    %section#slider
      / slider
      .container
        .row
          .col-sm-12
            #slider-carousel.carousel.slide{"data-ride" => "carousel"}
              %ol.carousel-indicators
                %li.active{"data-slide-to" => "0", "data-target" => "#slider-carousel"}
                %li{"data-slide-to" => "1", "data-target" => "#slider-carousel"}
                %li{"data-slide-to" => "2", "data-target" => "#slider-carousel"}
              .carousel-inner
                .item.active
                  .col-sm-6
                    %h1
                      %span> E
                      \-SHOPPER
                    %h2 Free E-Commerce Template
                    %p Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                    %button.btn.btn-default.get{type: "button"} Get it now
                  .col-sm-6
                    = image_tag "home/girl1.jpg", class: "girl img-responsive", alt: "" 
                    = image_tag "home/pricing.png" , class: "pricing", alt: "" 
                .item
                  .col-sm-6
                    %h1
                      %span> E
                      \-SHOPPER
                    %h2 100% Responsive Design
                    %p Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                    %button.btn.btn-default.get{type: "button"} Get it now
                  .col-sm-6
                    = image_tag "home/girl2.jpg", class: "girl img-responsive", alt: "" 
                    = image_tag "home/pricing.png" , class: "pricing", alt: "" 
                .item
                  .col-sm-6
                    %h1
                      %span> E
                      \-SHOPPER
                    %h2 Free Ecommerce Template
                    %p Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                    %button.btn.btn-default.get{type: "button"} Get it now
                  .col-sm-6
                    = image_tag "home/girl3.jpg", class: "girl img-responsive", alt: "" 
                    = image_tag "home/pricing.png" , class: "pricing", alt: "" 
                = link_to("#slider-carousel", data: { slide: "prev" }, class: "left control-carousel hidden-xs") do
                  %i.fa.fa-angle-left
                = link_to("#slider-carousel", data: { slide: "next" }, class: "right control-carousel hidden-xs") do
                  %i.fa.fa-angle-right     
    -# %script{src: "jquery.scrollUp.min.js"}
    -# %script{src: "price-range.js"}